{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds serve",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "serve",
        "--with-mocks",
        "--in-memory?"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Run zcom_capRegulationComplianceTransactionalService-1",
      "command": "cds-ts watch --profile hybrid",
      "request": "launch",
      "internalConsoleOptions": "openOnSessionStart",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/env/.env3",
      "preLaunchTask": "dev-project-cap",
      "env": {
        "run.config": "{\"handlerId\":\"lcap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/CHS_JDC_Project/zcom_capRegulationComplianceTransactionalService\\\",\\\"profileName\\\":\\\"\\\"}\"}"
      }
    },
    {
      "name": "Run TransactionalService",
      "command": "cds-ts watch --profile hybrid",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/.env4",
      "env": {
        "NODE_ENV": "development",
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/TransactionalService\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
      }
    }
  ]
}