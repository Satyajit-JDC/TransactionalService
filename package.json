  "scripts": {
    "start": "cds-serve",
    "start-service:ts": "cds-ts run --with-mocks --in-memory",
    "build:cf": "npm run build:cds && npm run cleanup:ts && npm run build:ts",
    "build:ts": "tsc",
    "build:cds": "cds build --production",
    "cleanup:ts": "npx rimraf gen/srv/**/*.ts",
    "cds-lint-tslint": "cds lint --debug && npx eslint .",
    "test": "cds bind --profile hybrid --exec jest",
    "undeploy": "cf undeploy zcom_capRegCompTransSrv --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-zcom_feregcompmadj": "cds watch --open zcom_feregcompmadj/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "cds": {
    "i18n": {
      "folders": [ "_i18n" ]
    },
    "requires": {
      "RegulationComplianceMasterService": {
        "kind": "odata-v4",
        "model": "srv/external/RegulationComplianceMasterService",
        "[hybrid]": {
          "credentials": {
            "path": "/service/RegulationComplianceMasterService",
            "destination": "RegulationComplianceMasterService"
          }
        },
        "[production]": {
          "credentials": {
            "path": "/service/RegulationComplianceMasterService",
            "destination": "RegulationComplianceMasterService"
          }
        }
      },
      "LogUtilityService": {
        "kind": "odata-v4",
        "model": "srv/external/LogUtilityService",
        "[hybrid]": {
          "credentials": {
            "path": "/odata/v4/log-utility",
            "destination": "LogUtilityService"
          }
        },
        "[production]": {
          "credentials": {
            "path": "/odata/v4/log-utility",
            "destination": "LogUtilityService"
          }
        }
      },
      "RenewableEvents": {
        "[hybrid]": {
          "kind": "enterprise-messaging-shared",
          "queue": {
            "name": "sap/dev/1/Renewable/Events"
          },
          "format": "cloudevents",
          "subscribePrefix": "sap/dev/1/"
        },
        "[production]": {
          "kind": "enterprise-messaging-shared",
          "queue": {
            "name": "sap/dev/1/Renewable/Events"
          },
          "format": "cloudevents",
          "subscribePrefix": "sap/dev/1/"
        }
      },
      "db": "hana",
      "auth": "xsuaa",
      "MaterialCharacteristics": {
        "kind": "odata",
        "model": "srv/external/MaterialCharacteristics",
        "[hybrid]": {
          "credentials": {
            "path": "/sap/opu/odata4/sap/zapi_materialcharcs_r_o4/srvd_a2x/sap/zmaterialcharacteristics_r/0001",
            "destination": "dn1clnt300-BAS-RINS"            
          }
        },
        "[production]": {
          "credentials": {
            "path": "/sap/opu/odata4/sap/zapi_materialcharcs_r_o4/srvd_a2x/sap/zmaterialcharacteristics_r/0001",
            "destination": "dn1clnt300-BAS-RINS"
          }
        }
      }
    },
    "cov2ap": {
      "plugin": true
    }
  },
  "sapux": [
    "app/zcom_feregcompmadj"
  ]
}